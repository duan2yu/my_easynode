{
        "apps": [
                {
                        "name": "CBB100-Sync",
                        "script": "main.js",
                        "args": [
                                "--debug-output=false",
                                "--src-dirs=projects/szzh/src",
                                "--main-class=szzh.cbb100.sync.CBB100SyncMain",
                                "--config-files=projects/szzh/etc/sync/cbb100-sync.conf",
                                "--bean-definitions=projects/szzh/etc/sync/cbb100-sync-beans.json"
                        ],
                        "pid_file": "CBB100-Sync.pid",
                        "instances": 1,
                        "watch": false,
                        "node_args": "--harmony",
                        "log_date_format": "",
                        "log_file": "../logs/CBB100-Sync.log",
                        "merge_logs": true,
                        "min_uptime": "30s",
                        "max_memory_restart": "1G",
                        "autorestart": true,
                        "cwd": "../../../bin",
                        "env": {
                        },
                        "next_gen_js": true
                }
        ]
}